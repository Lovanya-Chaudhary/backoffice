	<parameter name="current_site_no" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT          rd.rtv_site_no|| '-'|| rd.rtv_no rtv_no_combined, rd.rtv_no ,rd.rtv_site_no ,rd.site_no ,rd.vendor_id ,
                v.name vendor_name, v.adr1 vendor_adr1, v.adr2 vendor_adr2, v.city vendor_city, v.state vendor_state, v.zip vendor_zip,
                rd.address_line1,rd.address_line2 ,rd.city  ,rd.state ,rd.zip,
                rd.inv_doc_no ,rd.tracking_no ,nvl(rd.comments_txt,' ') comments_txt,rd.return_type_cd,nvl(rd.rma_id,' ') doc_rma_id,
                rd.ship_via, rd.ship_cost_amt, rd.containers_no, rd.tracking_no,
	      rl.seq_no, rl.labor_claim_no, rl.labor_claim_amt, rl.item_claim_cd, rl.item_serial_no, rl.orig_order_no,
                decode($P{line_item_info},'Y','Original Sales Order: ' || rl.orig_order_site_no ||'-' || rl.orig_order_no || '  Return Sales Order: ' || rl.ret_order_site_no || '-' || rl.ret_order_no) order_info,
                decode($P{line_item_info},'Y','Claim Form #: ' || rl.labor_claim_no || '   WLC Auth #: ' || rl.labor_claim_auth_no) labor_claim_str,
	      decode($P{line_item_info},'Y','Labor Claim Amt: ' || to_char(rl.labor_claim_amt,'$999,999.00') || '  Customer Received Credit: ' || rl.cust_recv_credit_fl) labor_claim_amt_str,
                decode($P{line_item_info},'Y','Claim Code: ' || rl.item_claim_cd || '  Failure Code: ' || rl.item_failure_cd || '  Ship date Code: ' || rl.item_ship_dt_cd) bat_claim_str,
                decode($P{line_item_info},'Y','Serial #: ' || rl.item_serial_no || '  Original Purchase Date: ' || to_char(rl.item_orig_purchase_dt)) bat_serial_str,
                rl.vendor_item, rl.sku_no, i.description, rl.type_cd, rl.rtv_qty, nvl(rl.rma_id,' ') rma_id,
                rl.po_site_no || '-' || rl.po_no po_no, rl.reason_cd, rl.cost_amt, rl.rtv_qty * rl.cost_amt ext_cost,
                decode($P{comments_line_item},'Y','Comments: ' || rl.comments_txt,'') line_comments,
	      b_item.get_item_id(rl.sku_no, $P{user_pref}) AS item_id ,
                s.full_name, s.adr1, s.adr2, s.city, s.state, s.zip ,
                decode($P{reason_codes},'Y',nvl(c.description,' '),' ') reason_description,
	      decode(rd.status_cd, 'N', 'New','I','Initiated','C','Shipped') status_str,
	      DECODE($P{current_site_no},NULL,NULL,
                	(select to_char(b_time_zone.site_time($P{current_site_no}, sysdate),tmxgbl.datetime_format) from dual)
                ) AS CURRENT_TIME_ZONE
            FROM rtvdoc   rd
            JOIN vendor v ON v.vendor_id = rd.vendor_id
            JOIN sitgrp sg ON sg.site_no = rd.rtv_site_no
            JOIN rtvlin rl ON rl.rtv_site_no = rd.rtv_site_no AND rl.rtv_no = rd.rtv_no
            JOIN invtory i ON i.sku_no = rl.sku_no
            JOIN site s ON s.site_no = rd.site_no
            LEFT OUTER JOIN catcode c ON c.cat_cd_no = rl.reason_cd  AND c.active_fl = 'Y' AND c.cat_no = 14
           WHERE sg.group_id = $P{site_group}
             AND rd.rtv_site_no = decode($P{from_rtv_site_no},null,rd.rtv_site_no,$P{from_rtv_site_no})
             AND rd.status_CD in ( 'N', 'I','C')
             AND rd.rtv_no >= DECODE($P{from_rtv_no},NULL,rd.rtv_no,$P{from_rtv_no})
             AND rd.rtv_no <= DECODE($P{to_rtv_no},NULL,rd.rtv_no,$P{to_rtv_no})
           ORDER BY rd.rtv_site_no,rd.rtv_no, rl.seq_no]]>
	</queryString>
