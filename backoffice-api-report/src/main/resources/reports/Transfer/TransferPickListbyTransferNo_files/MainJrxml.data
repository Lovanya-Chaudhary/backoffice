	<parameter name="sort_transfer_items_byWhere" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{sort_transfer_items_by}.equals("S") 	   ? " plist_seq_no, " :
	$P{sort_transfer_items_by}.equals("N") ? " plist_loc1, plist_shelf, plist_placement, plist_attr1_val asc, plist_price_list_seq asc, plist_mfg_cd, " :
