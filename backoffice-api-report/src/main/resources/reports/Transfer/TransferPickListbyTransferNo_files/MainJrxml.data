	$P{sort_transfer_items_by}.equals("V") ? " plist_vendor_id, " : " "]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT 
	plist_xfer_site_no
	,plist_xfer_no
   ,plist_ship_site
   ,plist_ship_desc
   ,plist_recv_site
   ,plist_recv_desc
   ,plist_prod_code
   ,plist_sku_no
   ,plist_seq_no
   ,plist_enter_qty
   ,plist_cost
   ,plist_repl_cost
   ,plist_sell_um
   ,plist_sku_desc
   ,plist_sku_lookup
   ,plist_wip
   ,plist_attr1_val
   ,plist_attr2_val
   ,plist_price_list_seq
   ,plist_mfg_cd
   ,plist_dept_no
   ,plist_class_no
   ,plist_line_no
   ,plist_pkg_desc
   ,plist_weighed_item_fl
   ,plist_loc1
   ,plist_shelf
   ,plist_placement
   ,plist_unit_price1
   ,plist_qoh
   ,plist_qor
   ,plist_upc
   ,plist_site_name
   ,plist_use_serial_fl
   ,plist_xfer_status
   ,plist_vendor_id 
   ,plist_vendor_name
   ,plist_vendor_item  
	,plist_reference
	,plist_tracking_cd
	,plist_comment_text
	,plist_xfer_barcode 
from (
SELECT  distinct xfh.xfer_site_no  AS plist_xfer_site_no
   ,xfh.xfer_no                   AS plist_xfer_no
   ,xfh.ship_site_no              AS plist_ship_site
   ,xfh.ship_desc					AS plist_ship_desc
   ,xfh.recv_site_no              AS plist_recv_site
   ,xfh.recv_desc					AS plist_recv_desc
   ,inv.dept_no||'/'||inv.class_no||'/'||inv.line_no as plist_prod_code
   ,inv.sku_no                    AS plist_sku_no
   ,xfd.seq_no                    AS plist_seq_no
   ,xfd.enter_qty                 AS plist_enter_qty
   ,xfd.cost                      AS plist_cost
   , decode((select config_fl from configflag where site_no = xfh.ship_site_no and name = 'XFER_USE_SHIP_SITE_REP_COST'),'Y'
 	 ,(select avg_cost.avg_cost(inv.sku_no,xfh.ship_site_no,'A') from dual),(select avg_cost.avg_cost(inv.sku_no,xfh.recv_site_no,'A') from dual)) AS plist_repl_cost
   ,inv.sell_um                   AS plist_sell_um
   ,inv.description               AS plist_sku_desc
   ,inv.lookup_desc               AS plist_sku_lookup
   ,inv.warehouse_inner_pack      AS plist_wip
   ,inv.attribute1_value                   AS plist_attr1_val
   ,inv.attribute2_value                   AS plist_attr2_val  
   ,inv.price_list_seq				AS plist_price_list_seq
   ,inv.mfg_cd                    AS plist_mfg_cd
   ,inv.dept_no                   AS plist_dept_no
   ,inv.class_no                  AS plist_class_no
   ,inv.line_no                   AS plist_line_no
   ,inv.pkg_desc                  AS plist_pkg_desc
   ,inv.weighed_item_fl           AS plist_weighed_item_fl
   ,il.location_id				AS plist_loc1
   ,il.shelf_id				AS plist_shelf
   ,il.placement_id			AS plist_placement
   ,ibs.unit_price1               AS plist_unit_price1
   ,ibs.qty_on_hand               AS plist_qoh
   ,ibs.qty_rsvd                  AS plist_qor
   ,ipc.upc_id                    AS plist_upc
   ,alt.name                      AS plist_site_name
   ,inv.use_serial_fl             AS plist_use_serial_fl
   ,xfh.xfer_status               AS plist_xfer_status
   ,b_vendor.get_primary_vendor(inv.sku_no,xfh.ship_site_no) AS plist_vendor_id 
   ,ven.name						AS plist_vendor_name
   ,b_item.get_item_id(inv.sku_no,'V')      AS plist_vendor_item  
,LPAD(xfh.xfer_site_no,4,0)
         ||LPAD(xfh.xfer_no     ,9,0)
         ||LPAD(xfd.sku_no      ,9,0)
         ||LPAD(xfd.seq_no      ,9,0) AS plist_reference
,xfh.tracking_cd AS plist_tracking_cd
,xfd.comment_text AS plist_comment_text
,xfh.xfer_barcode	AS plist_xfer_barcode
  FROM XFER_HDR xfh
JOIN XFER_DTL xfd ON xfd.xfer_site_no = xfh.xfer_site_no AND xfd.xfer_no = xfh.xfer_no
JOIN ALTSITE  alt ON alt.site_no  = xfh.recv_site_no
JOIN INVTORY  inv ON inv.sku_no = xfd.sku_no
JOIN INVBYSIT ibs ON ibs.sku_no = inv.sku_no AND ibs.site_no = alt.site_no  
JOIN ITEMUPC  ipc ON ipc.sku_no = inv.sku_no AND ipc.upc_seq = 1 
LEFT OUTER JOIN ITEM_LOCATION il ON  xfd.xfer_site_no = il.site_no AND xfd.sku_no = il.sku_no
LEFT OUTER JOIN VENDOR ven ON ven.vendor_id = b_vendor.get_primary_vendor(inv.sku_no,xfh.ship_site_no)
 WHERE 1=1
		AND $P{sort_transfer_items_by} in ('L','N')
       AND (($P{from_xfer_site_no} is null and $P{to_xfer_site_no} is null
            	AND xfh.xfer_no = xfh.xfer_no)
        	OR ($P{from_xfer_site_no} is null and $P{to_xfer_site_no} is not null
            	AND (xfh.xfer_site_no < $P{to_xfer_site_no} AND xfh.xfer_no = xfh.xfer_no
                	OR xfh.xfer_site_no = $P{to_xfer_site_no} AND xfh.xfer_no <= $P{to_xfer_no})) 
       		OR ($P{from_xfer_site_no} is not null and $P{to_xfer_site_no} is not null 
				AND $P{from_xfer_site_no} = $P{to_xfer_site_no} 
				AND xfh.xfer_site_no = $P{from_xfer_site_no}
					AND xfh.xfer_no   >= $P{from_xfer_no} AND xfh.xfer_no <= $P{to_xfer_no})
       		OR ($P{from_xfer_site_no} is not null and $P{to_xfer_site_no} is not null 
				AND $P{from_xfer_site_no} <> $P{to_xfer_site_no}
					AND ((xfh.xfer_site_no = $P{from_xfer_site_no} AND xfh.xfer_no   >= $P{from_xfer_no})
						or
               			(xfh.xfer_site_no = $P{to_xfer_site_no} AND xfh.xfer_no   <= $P{to_xfer_no})
               			or
               			(xfh.xfer_site_no > $P{from_xfer_site_no} and xfh.xfer_site_no < $P{to_xfer_site_no} AND xfh.xfer_no = xfh.xfer_no))))
       AND ($P{ship_site_no} IS NULL 
       		OR ($P{ship_site_no} IS NOT NULL AND xfh.ship_site_no  = $P{ship_site_no}))
       	AND ($P{recv_site_no} IS NULL 
       		OR ($P{recv_site_no} IS NOT NULL AND xfh.recv_site_no  in ($P!{recv_site_no})))
       AND xfh.xfer_status    NOT IN ('S', 'X', 'C')
UNION
SELECT  distinct xfh.xfer_site_no  AS plist_xfer_site_no
   ,xfh.xfer_no                   AS plist_xfer_no
   ,xfh.ship_site_no              AS plist_ship_site
   ,xfh.ship_desc					AS plist_ship_desc
   ,xfh.recv_site_no              AS plist_recv_site
   ,xfh.recv_desc					AS plist_recv_desc
   ,inv.dept_no||'/'||inv.class_no||'/'||inv.line_no as plist_prod_code
   ,inv.sku_no                    AS plist_sku_no
   ,xfd.seq_no                    AS plist_seq_no
   ,xfd.enter_qty                 AS plist_enter_qty
   ,xfd.cost                      AS plist_cost
   ,decode((select config_fl from configflag where site_no = xfh.ship_site_no and name = 'XFER_USE_SHIP_SITE_REP_COST'),'Y'
 	 ,(select avg_cost.avg_cost(inv.sku_no,xfh.ship_site_no,'A') from dual),(select avg_cost.avg_cost(inv.sku_no,xfh.recv_site_no,'A') from dual)) AS plist_repl_cost
   ,inv.sell_um                   AS plist_sell_um
   ,inv.description               AS plist_sku_desc
   ,inv.lookup_desc               AS plist_sku_lookup
   ,inv.warehouse_inner_pack      AS plist_wip
   ,inv.attribute1_value                   AS plist_attr1_val
   ,inv.attribute2_value                   AS plist_attr2_val  
   ,inv.price_list_seq				AS plist_price_list_seq
   ,inv.mfg_cd                    AS plist_mfg_cd
   ,inv.dept_no                   AS plist_dept_no
   ,inv.class_no                  AS plist_class_no
   ,inv.line_no                   AS plist_line_no
   ,inv.pkg_desc                  AS plist_pkg_desc
   ,inv.weighed_item_fl           AS plist_weighed_item_fl
   ,' '									AS plist_loc1
   ,' '									AS plist_shelf
   ,' '									AS plist_placement
   ,ibs.unit_price1               AS plist_unit_price1
   ,ibs.qty_on_hand               AS plist_qoh
   ,ibs.qty_rsvd                  AS plist_qor
   ,ipc.upc_id                    AS plist_upc
   ,alt.name                      AS plist_site_name
   ,inv.use_serial_fl             AS plist_use_serial_fl
   ,xfh.xfer_status               AS plist_xfer_status
   ,b_vendor.get_primary_vendor(inv.sku_no,xfh.ship_site_no) AS plist_vendor_id 
   ,ven.name						AS plist_vendor_name
   ,b_item.get_item_id(inv.sku_no,'V')      AS plist_vendor_item  
,LPAD(xfh.xfer_site_no,4,0)
         ||LPAD(xfh.xfer_no     ,9,0)
         ||LPAD(xfd.sku_no      ,9,0)
         ||LPAD(xfd.seq_no      ,9,0) AS plist_reference
,xfh.tracking_cd AS plist_tracking_cd
,xfd.comment_text AS plist_comment_text
,xfh.xfer_barcode	AS plist_xfer_barcode
  FROM XFER_HDR xfh
JOIN XFER_DTL xfd ON xfd.xfer_site_no = xfh.xfer_site_no AND xfd.xfer_no = xfh.xfer_no
JOIN ALTSITE  alt ON alt.site_no  = xfh.recv_site_no
JOIN INVTORY  inv ON inv.sku_no = xfd.sku_no
JOIN INVBYSIT ibs ON ibs.sku_no = inv.sku_no AND ibs.site_no = alt.site_no  
JOIN ITEMUPC  ipc ON ipc.sku_no = inv.sku_no AND ipc.upc_seq = 1 
LEFT OUTER JOIN ITEM_LOCATION il ON  xfd.xfer_site_no = il.site_no AND xfd.sku_no = il.sku_no
LEFT OUTER JOIN VENDOR ven ON ven.vendor_id = b_vendor.get_primary_vendor(inv.sku_no,xfh.ship_site_no)
 WHERE 1=1
		AND $P{sort_transfer_items_by} not in ('L','N')
       AND (($P{from_xfer_site_no} is null and $P{to_xfer_site_no} is null
            	AND xfh.xfer_no = xfh.xfer_no)
        	OR ($P{from_xfer_site_no} is null and $P{to_xfer_site_no} is not null
            	AND (xfh.xfer_site_no < $P{to_xfer_site_no} AND xfh.xfer_no = xfh.xfer_no
                	OR xfh.xfer_site_no = $P{to_xfer_site_no} AND xfh.xfer_no <= $P{to_xfer_no})) 
       		OR ($P{from_xfer_site_no} is not null and $P{to_xfer_site_no} is not null 
				AND $P{from_xfer_site_no} = $P{to_xfer_site_no} 
				AND xfh.xfer_site_no = $P{from_xfer_site_no}
					AND xfh.xfer_no   >= $P{from_xfer_no} AND xfh.xfer_no <= $P{to_xfer_no})
       		OR ($P{from_xfer_site_no} is not null and $P{to_xfer_site_no} is not null 
				AND $P{from_xfer_site_no} <> $P{to_xfer_site_no}
					AND ((xfh.xfer_site_no = $P{from_xfer_site_no} AND xfh.xfer_no   >= $P{from_xfer_no})
						or
               			(xfh.xfer_site_no = $P{to_xfer_site_no} AND xfh.xfer_no   <= $P{to_xfer_no})
               			or
               			(xfh.xfer_site_no > $P{from_xfer_site_no} and xfh.xfer_site_no < $P{to_xfer_site_no} AND xfh.xfer_no = xfh.xfer_no))))

       AND ($P{ship_site_no} IS NULL 
       		OR ($P{ship_site_no} IS NOT NULL AND xfh.ship_site_no  = $P{ship_site_no}))
       AND ($P{recv_site_no} IS NULL 
       		OR ($P{recv_site_no} IS NOT NULL AND xfh.recv_site_no  in ($P!{recv_site_no})))
       AND xfh.xfer_status    NOT IN ('S', 'X', 'C'))
order by plist_xfer_site_no,plist_xfer_no,
$P!{sort_transfer_items_byWhere}

     CASE       WHEN $P{sort_identifier_by} = 'S' THEN plist_sku_no END,
     CASE       WHEN $P{sort_identifier_by} = 'U' THEN plist_upc END,
     CASE       WHEN $P{sort_identifier_by} = 'M' THEN plist_mfg_cd END,
     CASE       WHEN $P{sort_identifier_by} = 'V' THEN plist_vendor_item 
END]]>
	</queryString>
