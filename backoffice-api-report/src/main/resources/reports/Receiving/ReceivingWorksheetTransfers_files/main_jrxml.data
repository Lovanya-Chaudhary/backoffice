	<parameter name="p_my_site" class="java.math.BigDecimal"/>
	<queryString language="SQL">
		<![CDATA[select xh.xfer_site_no, xh.xfer_no, xh.recv_site_no, xh.recv_desc, xh.ship_site_no, xh.ship_desc
,xh.ship_dt, nvl(xh.comment_text,' ') hdr_comment_text 
,xd.sku_no, xd.seq_no, xd.ship_qty, xd.comment_text dtl_comment_text
,inv.description, inv.mfg_cd, inv.sell_um, inv.lcu_qty
,xd.cost ,xd.orig_cost, xd.retail_amt
,iu.upc_id, rd.site_no rcvdoc_site_no, rd.doc_no rcvdoc_doc_no
, s.full_name, s.adr1,  s.city, s.state, s.zip
,vi.vendor_item
, nvl(rl.received_qty,0) received_qty
from xfer_hdr xh
JOIN xfer_dtl xd ON xd.xfer_site_no = xh.xfer_site_no and xd.xfer_no = xh.xfer_no
JOIN invtory inv ON inv.sku_no = xd.sku_no
JOIN itemupc iu ON iu.sku_no = inv.sku_no and iu.upc_seq = 1
JOIN site s ON s.site_no = $P{p_my_site}
JOIN entity_ownership eo ON eo.owner_id = s.owner_id
JOIN site s2 ON s2.site_no = xh.recv_site_no
JOIN entity_ownership eo2 ON eo2.owner_id = s2.owner_id
LEFT OUTER JOIN rcvdoc rd ON rd.type = 'T' and rd.po_site_no = xh.xfer_site_no and rd.po_no = xh.xfer_no
LEFT OUTER JOIN rcvlin rl ON rl.site_no = rd.site_no and rl.doc_no = rd.doc_no and rl.seq_no = xd.seq_no
LEFT OUTER JOIN vendor_item vi ON vi.sku_no = xd.sku_no and vi.site_no = xh.recv_site_no and ven_type = 'P'
where 1=1
and ((($P{p_from_xfer_site_no} is not null and $P{p_to_xfer_site_no} is not null 
      		and $P{p_from_xfer_no} is not null and $P{p_to_xfer_no} is not null
      		and $P{p_from_xfer_site_no} <> $P{p_to_xfer_site_no})
      		and ((xh.xfer_site_no = $P{p_from_xfer_site_no} and xh.xfer_no >= $P{p_from_xfer_no})
            or (xh.xfer_site_no > $P{p_from_xfer_site_no} and xh.xfer_site_no < $P{p_to_xfer_site_no} and xh.xfer_no = xh.xfer_no)
        		or (xh.xfer_site_no = $P{p_to_xfer_site_no} and xh.xfer_no <= $P{p_to_xfer_no})))
	or
	 (((($P{p_from_xfer_no} is null or $P{p_to_xfer_no} is null) and xh.xfer_site_no = xh.xfer_site_no)
    	or (xh.xfer_site_no >= $P{p_from_xfer_site_no} and xh.xfer_site_no <= $P{p_to_xfer_site_no}))
	and (($P{p_from_xfer_no} is not null and xh.xfer_no >= $P{p_from_xfer_no})
   	 	or ($P{p_from_xfer_no} is null and xh.xfer_no >= xh.xfer_no))
	and (($P{p_to_xfer_no} is not null and xh.xfer_no <= $P{p_to_xfer_no})
    	or ($P{p_to_xfer_no} is null and xh.xfer_no <= xh.xfer_no))))
and xh.xfer_status in ('S')
and (s.pkg_type = 'M'
	 or ($P{p_my_site} = xh.recv_site_no
     or ($P{p_my_site} = eo.hq_site_no and ($P{p_my_site} = eo2.hq_site_no))))
order by xh.xfer_site_no, xh.xfer_no,
CASE    WHEN $P{p_sort_by} = 'S' THEN inv.sku_no    END ,
CASE    WHEN $P{p_sort_by} = 'V' then vi.vendor_item   END ,
CASE    WHEN $P{p_sort_by} = 'L' then xd.seq_no   END,
CASE    WHEN $P{p_sort_by} = 'U' then iu.upc_id   END,
CASE    WHEN $P{p_sort_by} = 'M' THEN inv.mfg_cd END]]>
	</queryString>
