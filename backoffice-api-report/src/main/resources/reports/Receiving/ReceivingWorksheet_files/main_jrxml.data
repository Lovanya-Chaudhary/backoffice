	<parameter name="p_user_pref" class="java.lang.String"/>
	<parameter name="p_my_site" class="java.math.BigDecimal"/>
	<queryString language="SQL">
		<![CDATA[select ph.po_site_no, ph.po_no, ph.ship_site_no
, s.full_name, s.adr1,  s.city, s.state, s.zip
,ph.expect_dt, ph.vendor_id, ph.remark_txt
,pd.sku_no, pd.seq_no, pd.order_qty, pd.item_notes
,inv.description, inv.mfg_cd
,vi.vendor_item,vi.pack_cost,vi.pack_qty,vi.pack_um
,iu.upc_id, rd.site_no rcvdoc_site_no, rd.doc_no rcvdoc_doc_no
, nvl(rl.received_qty,0) received_qty
from po_hdr ph
JOIN po_dtl pd ON pd.po_site_no = ph.po_site_no and pd.po_no = ph.po_no
JOIN invtory inv ON inv.sku_no = pd.sku_no
JOIN vendor ven ON ven.vendor_id = ph.vendor_id 
JOIN vendor_item vi ON vi.seq_no = pd.vendor_item_seq_no
JOIN itemupc iu ON iu.sku_no = inv.sku_no and iu.upc_seq = 1
JOIN site s ON s.site_no = ph.ship_site_no
LEFT OUTER JOIN rcvdoc rd ON  rd.type = 'P' and rd.po_site_no = ph.po_site_no and rd.po_no = ph.po_no
LEFT OUTER JOIN rcvlin rl ON rl.site_no = rd.site_no and rl.doc_no = rd.doc_no and rl.seq_no = pd.seq_no
where 1=1
and ((($P{p_from_po_no} is null or $P{p_to_po_no} is null) and ph.po_site_no = ph.po_site_no)
    or (ph.po_site_no >= $P{p_from_po_site_no} and ph.po_site_no <= $P{p_to_po_site_no}))
and (($P{p_from_po_no} is not null and ph.po_no >= $P{p_from_po_no})
    or ($P{p_from_po_no} is null and ph.po_no >= ph.po_no))
and (($P{p_to_po_no} is not null and ph.po_no <= $P{p_to_po_no})
    or ($P{p_to_po_no} is null and ph.po_no <= ph.po_no))
and ph.po_status in ('O','P')
and ($P{p_my_site} = ph.ship_site_no or $P{p_my_site} = ph.po_site_no)
and ($P{p_lines_not_filled} = 'N' or ($P{p_lines_not_filled} = 'Y' and pd.order_qty > nvl(rl.received_qty,0)))
order by ph.po_site_no, ph.po_no,
CASE    WHEN $P{p_sort_by} = 'S' THEN inv.sku_no    END ,
CASE    WHEN $P{p_sort_by} = 'V' then pd.vendor_item   END ,
CASE    WHEN $P{p_sort_by} = 'L' then pd.seq_no   END,
CASE    WHEN $P{p_sort_by} = 'U' then iu.upc_id   END,
CASE    WHEN $P{p_sort_by} = 'M' THEN inv.mfg_cd END]]>
	</queryString>
