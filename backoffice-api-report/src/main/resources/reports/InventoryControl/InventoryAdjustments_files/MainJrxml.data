   $P{sort_by}.equals("V") ? " vi.vendor_item " : " inv.sku_no "]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select a.adjust_dt, a.sku_no, a.adj_site_no, a.adj_csite_no 
, a.adj_type, a.reason_cd, a.tran_time, a.adj_who, a.adj_comm, a.link_site_no, a.link_reference_no
,a.vendor_id, to_char(a.date_created,'hh:mi PM') adj_time
,a.serial_no
,ven.name, cc.description cc_desc
,dept.description dept_desc, class.description class_desc, line.description line_desc
,CASE 
	when a.adj_type = 'A' then 'Shrinkage'
	when a.adj_type = 'S' then 'Sales'
	when a.adj_type = 'Q' then 'Quantity'
	when a.adj_type = 'R' then 'Receiving'
	ELSE a.adj_type
END ADJ_TYPE_STR
,CASE 
	when a.adj_type = 'A' then a.shrinkage_units
	when a.adj_type = 'S' then a.sales_units+a.markdown_units+a.promo_units-a.returns_units
	when a.adj_type = 'Q' then a.qty_adj_units
    when a.adj_type = 'R' then DECODE(a.receive_units,0,a.shrinkage_units,a.receive_units)
	ELSE 0
END UNITS
,CASE 
	when a.adj_type = 'A' then a.shrinkage_amt
	when a.adj_type = 'S' then a.sales_amt+a.markdown_amt+a.promo_amt-a.returns_amt
	when a.adj_type = 'Q' then a.qty_adj_amt
	when a.adj_type = 'R' then DECODE(a.receive_amt,0,a.shrinkage_amt,a.receive_amt)
	ELSE 0
END AMT
,CASE 
	when a.adj_type = 'A' then a.shrinkage_amt
	when a.adj_type = 'S' then a.cost
	when a.adj_type = 'Q' then a.qty_adj_amt
	when a.adj_type = 'R' then DECODE(a.receive_amt,0,a.shrinkage_amt,a.receive_amt)
	ELSE 0
END COST_AMT
,inv.description, inv.mfg_cd,inv.dept_no, inv.class_no, inv.line_no
,inv.dept_no||'-' ||inv.class_no||'-'||inv.line_no  as sku_dcl
,iu.upc_id
,vi.vendor_item
,s.full_name
,b_item.get_item_id(inv.sku_no, $P{user_pref}) AS adj_item_id
from adjust a
LEFT OUTER JOIN invtory inv ON inv.sku_no = a.sku_no 
LEFT OUTER JOIN itemupc iu ON iu.sku_no = a.sku_no and iu.upc_seq = 1 --only get first
LEFT OUTER JOIN vendor ven ON ven.vendor_id = a.vendor_id
LEFT OUTER JOIN vendor_item vi ON vi.site_no = a.adj_site_no and vi.sku_no = a.sku_no and vi.ven_type = 'P'
LEFT OUTER JOIN catcode cc ON cc.cat_type = 'R' 
                                          and (adj_type = 'A' and cc.cat_no = 1
                                              or adj_type = 'Q' and cc.cat_no = 2
                                              or adj_type = 'S' and cc.cat_no = 3
                                              or adj_type = 'R' and cc.cat_no = 4)
                                          and cat_cd_no = a.reason_cd
LEFT OUTER JOIN dept ON dept.dept_no = $P{dept_no}
LEFT OUTER JOIN class ON class.dept_no = $P{dept_no} and class.class_no = $P{class_no}
LEFT OUTER JOIN line ON line.dept_no = $P{dept_no} and line.class_no = $P{class_no} and line.line_no = $P{line_no}
JOIN site s ON s.site_no = a.adj_site_no
where 1=1
     and (($P{from_seq_no} is not null and $P{to_seq_no} is not null
     	and a.adj_site_no = $P{site_group}
     	and a.adjust_dt = nvl($P{from_date},a.adjust_dt)
        and a.seq_no >= $P{from_seq_no} and a.seq_no <= $P{to_seq_no})
     or  (($P{from_seq_no} is null and $P{to_seq_no} is null)
     and a.adj_site_no in (select site_no from sitgrp_all where group_id = nvl($P{site_group},'ALL'))
     and (a.adjust_dt >= nvl($P{from_date},a.adjust_dt))
     and (a.adjust_dt <= nvl($P{to_date},a.adjust_dt))
     and (($P{deptgrp_no} is not null and inv.dept_no in (select dept_no from dept where deptgrp_no = $P{deptgrp_no}))
        or ($P{deptgrp_no} is null
          and
              (($P{dept_no} is not null and $P{class_no} is not null and $P{line_no} is not null 
                 and inv.dept_no = $P{dept_no} and inv.class_no = $P{class_no} and inv.line_no = $P{line_no})
           or
              ($P{dept_no} is not null and $P{class_no} is not null and $P{line_no} is null 
                 and inv.dept_no = $P{dept_no} and inv.class_no = $P{class_no})
           or
               ($P{dept_no} is not null and $P{class_no} is null and $P{line_no} is null 
                 and inv.dept_no = $P{dept_no})
           or
              ($P{dept_no} is null and $P{class_no} is null and $P{line_no} is null 
                 and 1=1))))

        and ((($P{rec_site_no} is not null) and ($P{rec_doc_no}  is not null)
           AND EXISTS (SELECT 'x'
              FROM rcvdoc rcd 
              WHERE a.link_site_no = rcd.site_no 
                  AND a.link_reference_no = rcd.doc_no 
                  AND rcd.site_no = $P{rec_site_no}  
                  AND rcd.doc_no = $P{rec_doc_no} ))
            or (($P{rec_site_no} is null) and ($P{rec_doc_no}  is null)
               and 1=1))
    --  PO Doc  
       and ((($P{po_site_no} is not null and $P{po_doc_no} is not null)
          AND EXISTS (SELECT 'x'
             FROM rcvdoc rcd  ,po_hdr poh 
             WHERE a.link_site_no = rcd.site_no
                AND a.link_reference_no = rcd.doc_no
                AND rcd.po_site_no = poh.po_site_no
                AND rcd.po_no = poh.po_no
                AND poh.po_site_no = $P{po_site_no}
                AND poh.po_no = $P{po_doc_no}))
          or (($P{po_site_no} is null and $P{po_doc_no} is null)
             and 1=1))
      and $P!{adj_typeWhere}
      
      ))
order by a.adj_site_no,a.adjust_dt desc, a.adj_type,$P!{sort_byWhere}]]>
	</queryString>
