	<queryString language="SQL">
		<![CDATA[SELECT vtm.vendor_id,
          inv.dept_no,
          inv.class_no,
          inv.line_no,
          cnt.sku_no,
          B_ITEM.GET_SKU_TYPE(CNT.SKU_NO, INV.LINK_SKU_NO, INV.WEIGHED_ITEM_FL) SKU_TYPE,
          b_item.get_item_id(inv.sku_no, $P{user_pref}) AS item_id,
          inv.description,
          inv.report_um,
          cnt.expected_qty exp_qty,
          inv.use_serial_fl,
          cnt.seq_no,  cnt.count_sheet_no, cnt.line_no cs_line_no,
          vtm.vendor_item,
          inv.mfg_cd ,
          (cnt.case_count*decode(vtm.pack_qty,null,inv.lcu_qty,vtm.pack_qty)) + cnt.sell_count AS count,
          (((cnt.case_count*decode(vtm.pack_qty,null,inv.lcu_qty,vtm.pack_qty)) + cnt.sell_count) - cnt.expected_qty) qty_disc,
          avg_cost.avg_cost(inv.sku_no,ch.site_no,DECODE(ct.config_txt,null,cr.config_txt,ct.config_txt)) avg_cost,
          (cnt.expected_qty * avg_cost.avg_cost(inv.sku_no,ch.site_no,DECODE(ct.config_txt,null,cr.config_txt,ct.config_txt))) expected_cost,
          (((cnt.case_count*decode(vtm.pack_qty,null,inv.lcu_qty,vtm.pack_qty)) + cnt.sell_count) * avg_cost.avg_cost(inv.sku_no,ch.site_no,DECODE(ct.config_txt,null,cr.config_txt,ct.config_txt))) actual_cost,
          ((((cnt.case_count*decode(vtm.pack_qty,null,inv.lcu_qty,vtm.pack_qty)) + cnt.sell_count) * avg_cost.avg_cost(inv.sku_no,ch.site_no,DECODE(ct.config_txt,null,cr.config_txt,ct.config_txt))) - (cnt.expected_qty * avg_cost.avg_cost(inv.sku_no,ch.site_no,DECODE(ct.config_txt,null,cr.config_txt,ct.config_txt)))) amt_disc,
		  d.description dept_desc,
		  c.description class_desc,
		  l.description line_desc,
		  ven.name ven_name,
		  inv.weighed_item_fl,
		  ch.count_date, ch.site_no
        FROM cntsheet_sku cnt
        JOIN cntsheet_hdr ch ON ch.seq_no = cnt.seq_no
        JOIN invtory inv ON inv.sku_no = cnt.sku_no
        JOIN dept d ON d.dept_no = inv.dept_no
        JOIN class c ON c.dept_no = inv.dept_no and c.class_no = inv.class_no
        JOIN line l ON l.dept_no = inv.dept_no and l.class_no = inv.class_no and l.line_no = inv.line_no
        LEFT OUTER JOIN vendor_item vtm ON vtm.site_no = ch.site_no and vtm.sku_no = cnt.sku_no
                                and (($P{sort_by} = 'V'
                                        and vtm.main_vendor_item_fl = 'Y' and vtm.vendor_id = nvl(ch.vendor_id,vtm.vendor_id))
                                    or ($P{sort_by} <> 'V' and vtm.ven_type = 'P'))
        LEFT OUTER JOIN vendor ven ON ven.vendor_id = vtm.vendor_id
        JOIN configrec cr ON cr.config_name = 'COST_METHOD'
        JOIN configtext ct ON ct.site_no = ch.site_no and ct.name = 'COST_METHOD'
        WHERE cnt.seq_no = $P{count_id}
              and (($P{discrepancy_only} = 'A' and 1=1)
                  or
                  ($P{discrepancy_only} = 'D' and ((cnt.case_count*decode(vtm.pack_qty,null,inv.lcu_qty,vtm.pack_qty)) + cnt.sell_count) <> cnt.expected_qty))
                   and (($P{exclude_inactive_items} =  'N' AND 1 = 1)
                    or  ($P{exclude_inactive_items} =  'Y' AND ((cnt.case_count*vtm.pack_qty) + cnt.sell_count) > 0 OR cnt.expected_qty <> 0))
       ORDER BY $P!{sort_byWhere}]]>
	</queryString>
