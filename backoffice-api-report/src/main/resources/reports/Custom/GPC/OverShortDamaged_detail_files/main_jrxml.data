	<parameter name="po_num" class="java.lang.String" isForPrompting="false"/>
	<parameter name="po_site_no" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT ah.site_no site_no, ah.doc_no doc_no, ph.vendor_id, name, 'R' rec_type, nvl(ah.asn_id,' ') asn_id,  1 orderby
 FROM po_hdr ph
JOIN   po_dtl pd ON pd.po_site_no = ph.po_site_no AND pd.po_no = ph.po_no  --AND pd.po_status != 'C'
JOIN rcvdoc rd ON rd.po_no = ph.po_no AND rd.po_site_no = ph.po_site_no
JOIN rcvlin rl ON rl.site_no = rd.site_no AND rl.doc_no = rd.doc_no AND rl.sku_no = pd.sku_no
join   invtory i ON i.sku_no = pd.sku_no
join   vendor v  ON v.vendor_id = ph.vendor_id
join asn_dtl ad on ad.source_doc_site_no = ph.po_site_no and ad.source_doc_no = ph.po_no and ad.source_doc_type_cd = 'P' and ad.sku_no = pd.sku_no and ad.source_doc_seq_no = pd.seq_no
join asn_hdr ah on ah.site_no = ad.site_no and ah.doc_no = ad.doc_no
WHERE 1=1
   AND (nvl($P{po_num},0) = 0 AND (($P{date_type} = 'R' AND rd.receive_dt BETWEEN $P{from_date} AND $P{to_date}
                               OR ($P{date_type} = 'P' AND ph.origin_dt BETWEEN $P{from_date} AND $P{to_date})))
                              AND ph.ship_site_no in (select site_no from sitgrp_all where group_id = $P{group_id})
    OR (nvl($P{po_num},0) > 0 AND ph.po_no = $P{po_num} AND ph.po_site_no = $P{po_site_no}))
   AND pd.order_qty != pd.received_qty
UNION
SELECT ph.po_site_no site_no, ph.po_no doc_no, ph.vendor_id, name, 'A' rec_type, nvl(r.asn_id,' ') asn_id,  2 orderby
 FROM po_hdr ph
join   vendor v  ON v.vendor_id = ph.vendor_id
JOIN   po_dtl pd ON pd.po_site_no = ph.po_site_no AND pd.po_no = ph.po_no
JOIN rcvdoc r ON r.po_no = ph.po_no AND r.po_site_no = ph.po_site_no
JOIN adjust a ON a.link_site_no = r.site_no AND a.link_reference_no = r.doc_no
JOIN rcvlin rl ON rl.doc_no = r.doc_no AND rl.sku_no = a.sku_no
JOIN invtory i ON i.sku_no = a.sku_no
WHERE 1=1
   AND a.adj_type = 'R'
   AND r.asn_id is not null
   AND (nvl($P{po_num},0) = 0 AND (($P{date_type} = 'R' AND r.receive_dt BETWEEN $P{from_date} AND $P{to_date}
                               OR ($P{date_type} = 'P' AND ph.origin_dt BETWEEN $P{from_date} AND $P{to_date})))
                              AND ph.ship_site_no in (select site_no from sitgrp_all where group_id = $P{group_id})
    OR (nvl($P{po_num},0) > 0 AND ph.po_no = $P{po_num} AND ph.po_site_no = $P{po_site_no}))


ORDER BY 3,1,6,7]]>
	</queryString>
