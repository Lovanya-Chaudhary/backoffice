	<queryString>
		<![CDATA[SELECT rl.rtv_site_no, rd.posted_dt rtv_dt, rl.sku_no, i.description,i.mfg_cd, rl.reason_cd,  c.description reason_description, ad.ship_qty asn_qty, ph.po_no, ph.po_site_no site_no,
              pd.order_qty*pd.pack_qty order_qty, (pd.order_qty*pd.pack_qty)-rtv_qty rtv_qty, -(rl.cost_amt*rl.rtv_qty) discrepancy_cost,
              nvl(i.attribute1_value,' ') product_line, nvl(i.attribute2_value,' ') group_code, nvl(ibs.class_code, ' ') classification
FROM rtvlin rl
JOIN po_hdr ph ON ph.po_site_no = rl.po_site_no AND ph.po_no = rl.po_no
JOIN po_dtl pd ON pd.po_site_no = rl.po_site_no AND pd.po_no = rl.po_no AND pd.sku_no = rl.sku_no
JOIN rtvdoc rd ON rd.rtv_no = rl.rtv_no AND rd.rtv_site_no = rl.rtv_site_no
JOIN invtory i ON i.sku_no = rl.sku_no
JOIN invbysit ibs ON ibs.sku_no = rl.sku_no and ibs.site_no = rl.po_site_no
JOIN catcode c ON c.cat_cd_no = rl.reason_cd AND c.cat_type = 'R' AND c.cat_no = 14
join asn_dtl ad on ad.source_doc_site_no = ph.po_site_no and ad.source_doc_no = ph.po_no and ad.source_doc_type_cd = 'P' and ad.sku_no = pd.sku_no and ad.source_doc_seq_no = pd.seq_no
WHERE rl.po_no = $P{po_no}
     AND rl.po_site_no = $P{po_site_no}
     AND (rl.cost_amt*rl.rtv_qty) != 0]]>
	</queryString>
