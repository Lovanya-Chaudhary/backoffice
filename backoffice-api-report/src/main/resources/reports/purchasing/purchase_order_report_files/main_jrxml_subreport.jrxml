	<queryString language="SQL">
		<![CDATA[select ph.po_site_no, ph.po_no, ph.ship_site_no, ph.bill_site_no,ph.po_status ph_status,
ph.master_po_site_no,ph.master_po_no, ph.po_type,
decode(ph.po_status,'F','FILLED',
       decode(ph.po_status,'O','OPEN',
       decode(ph.po_status,'C','CANCELLED',
       decode(ph.po_status,'N','NOT POSTED',
       decode(ph.po_status,'M','MASTER',
       decode(ph.po_status,'P','PARTIAL',ph.po_status)))))) po_stat,
ph.user_created, ph.order_account_id, ph.po_policy,
ph.origin_dt, ph.required_dt, ph.expect_dt, ph.cancel_dt,
ph.ship_terms, ph.fob, ph.terms, ph.employee_id, emp.full_name emp_full_name, ph.confirm_by, ph.remark_txt, ph.ship_inst_txt, ph.notes hdr_notes,
pd.seq_no, pd.order_qty, pd.pack_qty ||'/' || pd.pack_um pack_um, pd.sku_no, pd.vendor_cost cost, vi.vendor_currency_pack_cost vi_pack_cost,
pd.item_notes,
pd.item_description description, inv.mfg_cd, nvl(inv.attribute1_value,' ') attribute1_value, nvl(inv.attribute2_value,' ') attribute2_value, nvl(inv.attribute3_value,' ') attribute3_value,
vi.vendor_item, ibs.qty_on_hand, ibs.qty_on_ord, ibs.qty_on_backord, ibs.price1,
iu.upc_id,  ct.config_txt DISCLAIMER,
ven.vendor_id, ven.name ven_name, ven.adr1 ven_a1, ven.adr2 ven_a2, ven.city ven_ct, ven.state ven_st, ven.zip ven_zip, ven.country ven_country, 
nvl(vs1.phone_no,nvl(vs2.phone_no,nvl(vs3.phone_no,ven.phone_no))) ven_ph,
nvl(vs1.phone2_no,nvl(vs2.phone2_no,nvl(vs3.phone2_no,ven.phone2_no))) ven_ph2,
nvl(vs1.fax_no,nvl(vs2.fax_no,nvl(vs3.fax_no,ven.fax_no))) ven_fax,
nvl(vs1.contact,nvl(vs2.contact,nvl(vs3.contact,ven.contact))) ven_contact,
nvl(vs1.email_address,nvl(vs2.email_address,nvl(vs3.email_address,ven.email_address))) ven_email,
s1.full_name po_site_fname, s1.adr1 po_adr1, s1.adr2 po_adr2, s1.city po_city,
s1.state po_state, s1.zip po_zip, s1.phone_no po_phone, s1.fax_no po_fax, s1.manager_name po_man_name, s1.email_address po_email,
s2.full_name ship_site_fname, s2.adr1 ship_adr1, s2.adr2 ship_adr2, s2.city ship_city,
s2.state ship_state, s2.zip ship_zip, s2.phone_no ship_phone, s2.fax_no ship_fax, s2.manager_name ship_man_name, s2.email_address ship_email,
s3.full_name bill_site_fname, s3.adr1 bill_adr1, s3.adr2 bill_adr2, s3.city bill_city,
s3.state bill_state, s3.zip bill_zip, s3.phone_no bill_phone, s3.fax_no bill_fax, s3.manager_name bill_man_name, s3.email_address bill_email
,cr1.config_txt po_date_req_txt
,cr2.config_txt po_date_exp_txt
,cr3.config_txt po_date_can_txt
,cr4.config_txt po_date_com_txt
,cr5.config_txt po_date_txt
,cr_at1.config_txt key_att1_desc
,cr_at2.config_txt key_att2_desc
,cr_at3.config_txt key_att3_desc
,nvl(drc.code_meaning,'Ship Via') SHIP_VIA
FROM po_hdr ph
JOIN  vendor ven ON ven.vendor_id = ph.vendor_id
JOIN  site s1 ON s1.site_no = ph.po_site_no
JOIN  site s2 ON s2.site_no = ph.ship_site_no
JOIN  site s3 ON s3.site_no = ph.bill_site_no
LEFT OUTER JOIN ven_site vs1 ON vs1.site_no = ph.ship_site_no AND vs1.vendor_id = ph.vendor_id
LEFT OUTER JOIN ven_site vs2 ON vs2.site_no = ph.bill_site_no AND vs2.vendor_id = ph.vendor_id
LEFT OUTER JOIN entity_ownership eo ON eo.owner_id = s2.owner_id
LEFT OUTER JOIN ven_site vs3 ON vs3.site_no = eo.hq_site_no AND vs3.vendor_id = ph.vendor_id
LEFT OUTER JOIN po_dtl pd ON pd.po_site_no = ph.po_site_no AND pd.po_no = ph.po_no AND pd.po_status <> 'C'
LEFT OUTER JOIN  invtory inv ON inv.sku_no = pd.sku_no
LEFT OUTER JOIN vendor_item vi ON vi.seq_no = pd.vendor_item_seq_no
LEFT OUTER JOIN invbysit ibs ON ibs.sku_no = inv.sku_no AND ibs.site_no = vi.site_no
LEFT OUTER JOIN itemupc iu ON iu.sku_no = inv.sku_no AND iu.upc_seq = 1
LEFT OUTER JOIN configtext ct ON ct.site_no = ibs.site_no AND ct.name = 'RF_PO_PRINT_DISCLAIMER'
LEFT OUTER JOIN configrec cr1 ON cr1.config_name = 'PO_DATE_REQUIRED_TXT'
LEFT OUTER JOIN configrec cr2 ON cr2.config_name = 'PO_DATE_EXPECTED_TXT'
LEFT OUTER JOIN configrec cr3 ON cr3.config_name = 'PO_DATE_CANCELED_TXT'
LEFT OUTER JOIN configrec cr4 ON cr4.config_name = 'PO_DATE_COMPLETED_TXT'
LEFT OUTER JOIN configrec cr5 ON cr5.config_name = 'PO_DATE_TXT'
LEFT OUTER JOIN configrec cr_at1 ON cr_at1.config_name = 'ATTRIBUTE1_DESC'
LEFT OUTER JOIN configrec cr_at2 ON cr_at2.config_name = 'ATTRIBUTE2_DESC'
LEFT OUTER JOIN configrec cr_at3 ON cr_at3.config_name = 'ATTRIBUTE3_DESC'
LEFT OUTER JOIN domain_ref_usage dru ON table_name = 'PO_HDR' and column_name = 'SHIP_VIA'
LEFT OUTER JOIN domain_ref_code drc   ON drc.domain_ref_id = dru.domain_ref_id and drc.code_value = ph.ship_via
LEFT OUTER JOIN employee emp ON emp.employee_id = ph.employee_id
WHERE  1=1
and ph.master_po_site_no = $P{p_master_po_site_no}
and ph.master_po_no = $P{p_master_po_no} and ph.po_type not in ('M','R')
and ph.origin_dt >= decode($P{p_from_date},null,origin_dt,$P{p_from_date})
and ph.origin_dt <= decode($P{p_to_date},null,origin_dt,$P{p_to_date})
and ph.vendor_id = decode($P{p_vendor_id},null,ph.vendor_id,$P{p_vendor_id})
and (decode(ph.employee_id,null,'~',ph.employee_id) 
     = decode($P{p_buyer_id},null,decode(ph.employee_id,null,'~',ph.employee_id),$P{p_buyer_id}))
and ph.ship_site_no = decode($P{p_ship_site_no},null,ph.ship_site_no,$P{p_ship_site_no})
and ph.po_status <> 'D'
order by ph.po_site_no, ph.po_no,
CASE    WHEN $P{p_sort_by} = 'S' THEN inv.sku_no    END ,
CASE    WHEN $P{p_sort_by} = 'V' then pd.vendor_item   END ,
CASE    WHEN $P{p_sort_by} = 'U' then iu.upc_id   END,
CASE    WHEN $P{p_sort_by} = 'M' THEN inv.mfg_cd END]]>
	</queryString>
