	<queryString>
		<![CDATA[select ph.po_site_no, ph.po_no, ph.ship_site_no, ph.bill_site_no,
decode(ph.po_status,'F','FILLED',
       decode(ph.po_status,'O','OPEN',
       decode(ph.po_status,'C','CANCELLED',
       decode(ph.po_status,'N','NEW',
       decode(ph.po_status,'M','MASTER',
       decode(ph.po_status,'P','PARTIAL',ph.po_status)))))) po_stat,
ph.user_created, ph.order_account_id, ph.po_policy,
ph.origin_dt, ph.required_dt, ph.expect_dt,
ph.ship_terms, ph.ship_via, ph.fob, ph.terms, ph.employee_id, ph.confirm_by, ph.remark_txt,
pd.seq_no, pd.order_qty, pd.pack_qty ||'/' || pd.pack_um pack_um, pd.sku_no, pd.cost,
pd.item_notes,
inv.description, inv.mfg_cd, vi.vendor_item, ibs.qty_on_hand, ibs.qty_on_ord, ibs.qty_on_backord, ibs.price1,
iu.upc_id,  (SELECT config_txt  FROM  configtext WHERE site_no = ibs.site_no AND  NAME =  'RF_PO_PRINT_DISCLAIMER') DISCLAIMER,
ven.vendor_id, ven.name ven_name, ven.adr1 ven_a1, ven.adr2 ven_a2, ven.city ven_ct, ven.state ven_st, ven.zip ven_zip, ven.country ven_country,
 nvl(vs1.phone_no,nvl(vs2.phone_no,nvl(vs3.phone_no,ven.phone_no))) ven_ph,
nvl(vs1.fax_no,nvl(vs2.fax_no,nvl(vs3.fax_no,ven.fax_no))) ven_fax,
s1.full_name po_site_fname,
s2.full_name ship_site_fname, s2.adr1 ship_adr1, s2.adr2 ship_adr2, s2.city ship_city, s2.state ship_state, s2.zip ship_zip, s2.phone_no ship_phone, s2.fax_no ship_fax,
s3.full_name bill_site_fname, s3.adr1 bill_adr1, s3.adr2 bill_adr2, s3.city bill_city, s3.state bill_state, s3.zip bill_zip, s3.phone_no bill_phone, s3.fax_no bill_fax
from po_hdr ph, po_dtl pd, vendor ven, site s1, site s2, site s3, invtory inv, vendor_item vi, invbysit ibs, itemupc iu, ven_site vs1, ven_site vs2, ven_site vs3
where ph.po_site_no = pd.po_site_no
and ph.po_no = pd.po_no
and ph.po_site_no = $P{p_po_site_no}
and ph.po_no >= $P{p_from_po_no}
and ph.po_no <= $P{p_to_po_no}
and origin_dt >= decode($P{p_from_dt},null,origin_dt,$P{p_from_dt})
and origin_dt <= decode($P{p_to_dt},null,origin_dt,$P{p_to_dt})
and s1.site_no = ph.po_site_no
and s2.site_no = ph.ship_site_no
and s3.site_no = ph.bill_site_no
and ph.vendor_id = ven.vendor_id
and iu.sku_no = inv.sku_no and iu.upc_seq = 1
and pd.sku_no = inv.sku_no
and vi.sku_no = inv.sku_no
and ibs.sku_no = inv.sku_no
and ibs.site_no = vi.site_no
and vi.site_no = ph.ship_site_no
and vi.vendor_id = ph.vendor_id
and ven.vendor_id = decode($P{p_vendor_id},null,ven.vendor_id,$P{p_vendor_id})
and nvl(ph.employee_id,'~') = decode($P{p_buyer_id},null,nvl(ph.employee_id,'~'),$P{p_buyer_id})
and ph.ship_site_no = decode($P{p_ship_site_no},null,ph.ship_site_no,$P{p_ship_site_no})
and (ph.po_status = $P{p_print_not_posted} or ph.po_status = $P{p_print_posted}
or ph.po_status = $P{p_print_partial} or ph.po_status = $P{p_print_filled}
or ph.po_status = $P{p_print_cancel} or ph.po_status = $P{p_print_Master})
and vs1.site_no = ph.ship_site_no and vs1.vendor_id = ph.vendor_id
and vs2.site_no = ph.bill_site_no and vs2.vendor_id = ph.vendor_id
and vs3.site_no = ph.po_site_no and vs3.vendor_id = ph.vendor_id
order by ph.po_site_no, ph.po_no,
CASE    WHEN $P{p_order_by} = 'L'  then pd.seq_no   END ,
CASE    WHEN $P{p_order_by} = 'K' THEN inv.sku_no    END ,
CASE    WHEN $P{p_order_by} =  'V' then vi.vendor_item   END,
CASE    WHEN $P{p_order_by} =  'U' then iu.upc_id   END,
CASE   WHEN $P{p_order_by} =  'M' THEN inv.mfg_cd END]]>
	</queryString>
