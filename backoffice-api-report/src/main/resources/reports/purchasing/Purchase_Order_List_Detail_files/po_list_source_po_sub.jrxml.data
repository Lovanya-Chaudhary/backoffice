	<parameter name="p_po_no" class="java.lang.String"/>
	<parameter name="p_po_site_no" class="java.lang.String"/>
	<parameter name="p_sku_no" class="java.math.BigDecimal"/>
	<queryString language="SQL">
		<![CDATA[select ph2.po_site_no source_po_site_no, ph2.po_no source_po_no, sum(pd.order_qty) total_order_qty
from po_hdr ph1
join po_hdr ph2 on ph1.po_site_no = ph2.master_po_site_no
                and ph1.po_no = ph2.master_po_no
join po_dtl pd on pd.po_site_no = ph2.po_site_no and pd.po_no = ph2.po_no
where ph1.po_site_no = $P{p_po_site_no}  and ph1.po_no = $P{p_po_no}
group by ph2.po_site_no, ph2.po_no]]>
	</queryString>
