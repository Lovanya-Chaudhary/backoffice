	<queryString language="SQL">
		<![CDATA[select ph.po_site_no, ph.po_no
, ph.origin_dt,ph.required_dt,expect_dt,complete_dt,cancel_dt,ph.vendor_id,ven.name ven_name
,ph.employee_id
, ph.ship_site_no, ph.bill_site_no, ph.fob, ph.confirm_by,ph.remark_txt
,ph.ship_via,ph.ship_terms, ph.terms
,ph.master_po_site_no,ph.master_po_no
,ph.account_no
,decode(ph.po_status,'F','Filled',
       decode(ph.po_status,'O','Open',
       decode(ph.po_status,'C','Cancelled',
       decode(ph.po_status,'N','Not Posted',
       decode(ph.po_status,'M','Master',
       decode(ph.po_status,'P','Partial',ph.po_status)))))) po_status
,decode(pd.po_status,'F','Filled',
       decode(pd.po_status,'O','Open',
       decode(pd.po_status,'C','Cancelled',
       decode(pd.po_status,'N','Not Postd',
       decode(pd.po_status,'M','Master',
       decode(pd.po_status,'P','Partial',pd.po_status)))))) po_dtl_status
,s1.full_name bill_site_desc, s2.full_name ship_site_desc
,pd.seq_no, pd.sku_no, pd.order_qty, pd.received_qty, backorder_qty
,pd.pack_qty,pd.pack_um,pd.cost, pd.filled_dt, pd.vendor_item, pd.adhdr_ad_id ad_id
,pd.item_description description, inv.mfg_cd, inv.gl_cat_id, ibs.unit_price1, iu.upc_id
from po_hdr ph
JOIN po_dtl pd ON pd.po_site_no = ph.po_site_no and pd.po_no = ph.po_no
JOIN invtory inv ON pd.sku_no = inv.sku_no
JOIN site s1 ON s1.site_no = ph.bill_site_no
JOIN site s2 ON s2.site_no = ph.ship_site_no
JOIN invbysit ibs ON ibs.site_no = ph.ship_site_no and ibs.sku_no = pd.sku_no
JOIN itemupc iu ON iu.sku_no = pd.sku_no and iu.upc_seq = 1
JOIN vendor ven ON ph.vendor_id  = ven.vendor_id
where 1=1
and ph.po_site_no = $P!{p_from_po_site_noWhere}
and ph.po_no >= $P!{p_from_po_noWhere}
and ph.po_no <= $P!{p_to_po_noWhere}
and ph.origin_dt >= decode($P{p_from_date},null,origin_dt,$P{p_from_date})
and ph.origin_dt <= decode($P{p_to_date},null,origin_dt,$P{p_to_date})
and ph.vendor_id = decode($P{p_vendor_id},null,ph.vendor_id,$P{p_vendor_id})
and (decode(ph.employee_id,null,'~',ph.employee_id) 
	    = decode($P{p_buyer_id},null,decode(ph.employee_id,null,'~',ph.employee_id),$P{p_buyer_id}))
and ph.ship_site_no = decode($P{p_ship_site_no},null,ph.ship_site_no,$P{p_ship_site_no})
and (decode(pd.adhdr_ad_id,null,'~',pd.adhdr_ad_id) = decode($P{p_ad_id},null,'~',$P{p_ad_id}))
and (($P{p_print_consolidated_orders} = 'N' and $P{p_print_direct_orders} = 'N' and $P{p_print_automated_orders} = 'N' and $P{p_print_internal_orders} = 'N')
	or (($P{p_print_consolidated_orders} = 'Y' or $P{p_print_direct_orders} = 'Y' or $P{p_print_automated_orders} = 'Y' or $P{p_print_internal_orders} = 'Y')
      and  (($P{p_print_consolidated_orders} = 'Y' and ph.po_type = 'C')
          or  ($P{p_print_direct_orders} = 'Y' and ph.po_type = 'D')
          or  ($P{p_print_automated_orders} = 'Y' and ph.po_type = 'E')
          or  ($P{p_print_internal_orders} = 'Y' and ph.po_type = 'I'))))
order by ph.po_site_no, ph.po_no,
CASE    WHEN $P{p_sort_by} = 'G' then inv.gl_cat_id   END ,
CASE    WHEN $P{p_sort_by} = 'L' then pd.seq_no   END ,
CASE    WHEN $P{p_sort_by} = 'S' THEN inv.sku_no    END ,
CASE    WHEN $P{p_sort_by} = 'V' then pd.vendor_item   END ,
CASE    WHEN $P{p_sort_by} = 'U' then iu.upc_id   END,
CASE    WHEN $P{p_sort_by} = 'M' THEN inv.mfg_cd END,
CASE    WHEN $P{p_sort_by} = 'A' THEN pd.adhdr_ad_id END]]>
	</queryString>
